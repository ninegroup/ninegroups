<script type="text/javascript">
//查询
function search(){
	var city=document.getElementById('destInput').value;
	var adress=document.getElementById('adress').value;
	location.href="<?php echo e(URL::route('Search/HomepageSearch')); ?>?city="+city+"&adress="+adress;
}
//价格排序
function houseprice(order){
	var city=document.getElementById('destInput').value;
	var adress=document.getElementById('adress').value;
	location.href="<?php echo e(URL::route('Search/HomepageSearch')); ?>?order="+order+"&city="+city+"&adress="+adress;
}
</script>