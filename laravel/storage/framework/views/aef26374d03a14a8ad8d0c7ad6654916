                            function getProductId() {
                                var fullUrl = location.href;
                                var pid = 0;
                                if (fullUrl.indexOf("#") > -1) {
                                    var params = fullUrl.substring(fullUrl.indexOf("#") + 1, fullUrl.length);
                                    var paramsArray = params.split("&");
                                    for (var i = 0; i < paramsArray.length; i++) {
                                        if (paramsArray[i].indexOf("pid") > -1) {
                                            if (paramsArray[i].split("=").length == 2) {
                                                pid = paramsArray[i].split("=")[1];
                                            }
                                        }
                                    }

                                }
                                return pid;
                            }

