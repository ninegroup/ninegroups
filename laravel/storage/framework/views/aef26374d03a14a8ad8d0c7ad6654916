    var gPrevStartDate;
    var gPreEndDate;

    var getProductUrl= "/UnitDetail/UnitProductPlus/" ;

    function txtStartDateChangeHandle(gtxtStartDate, gtxtEndDate) {
        var vtmpStartDate=gtxtStartDate.val();
        var vtmpEndDate=gtxtEndDate.val();
        if (!vtmpStartDate && !vtmpEndDate) {
            return;
        }
        if (!vtmpEndDate) {
            var tmpDate = new Date(vtmpStartDate);
            tmpDate.setDate(tmpDate.getDate()+1);
            vtmpEndDate=tmpDate.Format("yyyy-MM-dd");
        }
        if (vtmpStartDate == gPrevStartDate) {
            return;
        }
        gPrevStartDate = vtmpStartDate;
        gPreEndDate=vtmpEndDate;
        //setSDFlag();
        //syncCalender();
    }
    function txtEndDateChangeHandle(gtxtStartDate, gtxtEndDate) {
        var vtmpStartDate=gtxtStartDate.val();
        var vtmpEndDate=gtxtEndDate.val();
        if (!vtmpStartDate && !vtmpEndDate) {
            return;
        }
        if (!vtmpStartDate) {
            var tmpDate = new Date(vtmpEndDate);
            tmpDate.setDate(tmpDate.getDate()-1);
            vtmpStartDate=tmpDate.Format("yyyy-MM-dd");
        }

        if (vtmpEndDate == gPreEndDate) {
            return;
        }
        gPrevStartDate = vtmpStartDate;
        gPreEndDate=vtmpEndDate;
        //setSDFlag();
    }
    var mapType=1;
    
    var isLogin = false;



