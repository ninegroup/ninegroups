                            if (Tj.Detail.LoadProduct) {
                                function refreshProduct(isInit){
                                    $.ajax({
                                        url: getProductUrl,
                                        data: {
                                            unitId: $("#UnitId").val(),
                                            productId: getProductId(),
                                            unitDetailSubType: Tj.Detail.Subtype,
                                            startTime: $("#unitDetailStartDate").val(),
                                            endTime: $("#unitDetailEndDate").val(),
                                            bookingCount: 1,
                                            prevId: (traceData && traceData.pageId) ? traceData.pageId : ''
                                        },
                                        type: "POST",
                                        beforeSend: function () { $("#productcontent").html('<div class="loading-wrap"><i class="icon-loading"></i></div>'); $("#bestProduct").html('<div class="loading-wrap"></div>'); },
                                        cache: false,
                                        dataType: "html",
                                        complete: function () { if(isInit) initScrollto(); },
                                        success: function (retData) {
                                            var res = $(retData);
                                            $("#productcontent").html(retData);
                                            if (res.attr("data-spring") !== undefined) {
                                                $(".h-tag").show();
                                            }
                                            $("#bestProduct").html($("#economicalProduct").html());
                                            $("#pricebox").html($("#economicalProductPrice").html());
                                            AddBestProductEvents();
                                        },
                                        error: function () {
                                            $("#productcontent").html('<div class="no-products" data-error="true" >在你选择的日期内没有可用产品</div>');
                                            $("#bestProduct").html('<div class="no-room-tips">此时段该管理公司不接受预订</div>');
                                        }
                                    });
                                }

                                $("#changeBtn").click(function () {
                                    refreshProduct();
                                });
                                refreshProduct(true);
                            }

                            $(document).delegate(".detail-btn", "click", null, function () {
                                var allcomments = $(this).siblings("span.j-unitcomment-allcomments");
                                if (allcomments.is(":visible")) {
                                    $(this).siblings("span.j-unitcomment-ellipsis").show();
                                    $(this).siblings("span.j-unitcomment-allcomments").hide();
                                    $(this).html("展开全部");
                                } else {
                                    $(this).siblings("span.j-unitcomment-ellipsis").hide();
                                    $(this).siblings("span.j-unitcomment-allcomments").show();
                                    $(this).html("收起全部");
                                }
                            });

                            var unitCalendarUrl = '/UnitDetail/GetUnitCalendar?unitId=3581&subType=None&beginDate=2016-05-27';
                            /*create default house status data*/
                            (function () {
                                var now = new Date();
                                var houseData = [];
                                var date;
                                var monthData = [];
                                for (var i = 1; i <= 12; i++) {
                                    monthData = [];
                                    date = new Date(now.getFullYear(), now.getMonth() + i, 0);
                                    for (var j = 0; j < date.getDate() ; j++) {
                                        monthData.push([1]);
                                    }
                                    houseData.push(monthData);
                                }
                                window.dateHouseStatus = houseData;
                            })();
